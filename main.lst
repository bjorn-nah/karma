    1                     ;-----------------------------------------------------------------------------
    2                     ; Send sound to psg for output
    3                     ;
    4                     ; X should hold voice to send....
    5                     
    6  02:D631            subOutputSounds
    7  02:D631  A2 05             ldx             #5
    8                             
    9                             ;..........................................................................
   10                             ; is voice on ?
   11                             
   12  02:D633            .voxLoop
   13  02:D633  BD 07 26          lda             psgSubVoiceStatus,x                     ; get voice status
   14  02:D636  C9 01             cmp             #1                                                              ; is it on ?
   15  02:D638  10 03             bpl             .voxOn                                                  ; yes, keep going
   16  02:D63A  4C E3 D6          jmp             .nextVoice                                              ; no, skip it
   17                             
   18                             ;..........................................................................
   19                             ; voice is on. check to see if it ended.
   20                     
   21  02:D63D            .voxOn  
   22                     
   23  02:D63D  8E 00 08          stx             psg_ch                                  ; select hardware voice
   24  02:D640  F0 1D             beq             .voxOK                                  ; if status is 0, main is playing
   25                     
   26                             ;..........................................................................
   27                             ; status is something other than 0 or 1. we assume that means the track ended.
   28                             
   29  02:D642  9C 04 08          stz             psg_ctrl                                ; turn channel off
   30  02:D645  9E 07 26          stz             psgSubVoiceStatus,x             ; turn voice off
   31                     
   32                             ;..........................................................................
   33                             ; voice is shutting off. is main still playing ?
   34                             
   35  02:D648  AD DF 22          lda             psgSystemNo                             ; check system number
   36  02:D64B  C9 01             cmp             #1                                              ; sub-track only. 
   37  02:D64D  F0 0D             beq             .skipSub                                ; leave voice off
   38                             
   39  02:D64F  BD 7C 24          lda             psgMainVoiceStatus,x    ; get main track state
   40  02:D652  F0 08             beq             .skipSub                                ; main off, leave track off
   41                     
   42                             ;..........................................................................
   43                             ; sub-track ended, but main should keep playing. restore waveform 
   44                             
   45  02:D654  BD 74 24          lda             psgMainWaveNo,x 
   46  02:D657  29 7F             and             #$7f
   47  02:D659  9D 74 24          sta             psgMainWaveNo,x
   48                             
   49  02:D65C            .skipSub
   50  02:D65C  4C E3 D6          jmp             .nextVoice                              ; track on, ignore it.
   51                             
   52                             ;..........................................................................
   53                             ; check main to see if we need wave change
   54                             
   55  02:D65F            .voxOK
   56                     
   57  02:D65F  BD FF 25          lda             psgSubWaveNo,x                  ; get wave number for voice
   58  02:D662  30 45             bmi             .waveOK                                 ; if bit 7 set, wave already loaded
   59                     
   60                             ;..........................................................................
   61                             ; mark wave as being loaded. We're gonna load it next
   62                             
   63  02:D664  09 80             ora             #$80                                    ; mark wave as loaded
   64  02:D666  9D FF 25          sta             psgSubWaveNo,x                  ; save it
   65  02:D669  29 7F             and             #$7F                                    ; resore original wave number
   66  02:D66B  C9 2D             cmp             #$2d                                    ; is it internal wave ?
   67  02:D66D  B0 12             bcs             .isUserWave                             ; nope, load user waveform
   68                             
   69                             ;..........................................................................
   70                             ; wave number is internal wave. load it.
   71                     
   72  02:D66F  20 E8 CC          jsr             getWaveOffset                   ; locate wave data
   73  02:D672  A9 4D             lda             #LOW( psgWaves )                ; add base address
   74  02:D674  18            clc
   75  02:D675  65 E8             adc             <psgPtr1                                        ; add low byte to offset
   76  02:D677  85 E8             sta             <psgPtr1
   77  02:D679  A9 D9             lda             #HIGH( psgWaves )
   78  02:D67B  65 E9             adc             <psgPtr1+1                              ; add high byte to offset
   79  02:D67D  85 E9             sta             <psgPtr1+1
   80  02:D67F  80 15             bra             .loadWave
   81                             
   82                             ;..........................................................................
   83                             ; wave number is user wave. locate and load it
   84                     
   85  02:D681            .isUserWave
   86                     
   87  02:D681  38                sec                                                             ; subtract internal wave numbers
   88  02:D682  E9 2D             sbc             #$2d
   89  02:D684  20 E8 CC          jsr             getWaveOffset                   ; calculate offset from user wave table
   90                     
   91  02:D687  AD D2 22          lda             psgUserWaveTable                ; low byte of user table
   92  02:D68A  18                clc
   93  02:D68B  65 E8             adc             <psgPtr1
   94  02:D68D  85 E8             sta             <psgPtr1
   95  02:D68F  AD D3 22          lda             psgUserWaveTable+1              ; high byte of user table
   96  02:D692  65 E9             adc             <psgPtr1+1
   97  02:D694  85 E9             sta             <psgPtr1+1
   98                             
   99                             ;..........................................................................
  100                             ; load wave to psg. psgPtr1 is address of wave data.
  101                             
  102  02:D696            .loadWave
  103                     
  104  02:D696  A9 40             lda             #PSG_DDA                                ; DDA bit in psg
  105  02:D698  8D 04 08          sta             psg_ctrl                                ; turn psg off
  106  02:D69B  9C 04 08          stz             psg_ctrl                                ; reset psg to load wave
  107                             
  108                             ; I don't see why they didn't use block instruction....
  109  02:D69E  C2                cly                                                             ; y is byte count
  110  02:D69F            .sendByte
  111  02:D69F  B1 E8             lda             [psgPtr1],y                             ; get value
  112  02:D6A1  8D 06 08          sta             psg_wavebuf                             ; save in psg
  113  02:D6A4  C8                iny
  114  02:D6A5  C0 20             cpy             #32                                             ; 32 bytes done ?
  115  02:D6A7  90 F6             bcc             .sendByte                               ; nope, loop
  116                             
  117                             ;..........................................................................
  118                             ; waveform all set. is voice in noise mode ?
  119                             
  120  02:D6A9            .waveOK
  121                     
  122  02:D6A9  E0 04             cpx             #4                                              ; is it noise-capable channel ?
  123  02:D6AB  90 0C             bcc             .notNoise                               ; nope, skip noise setup
  124                             
  125  02:D6AD  BD 01 26          lda             psgSubNoiseFlag-4,x             ; check for noise
  126  02:D6B0  30 07             bmi             .notNoise                               ; if bit 7 set, se've already set noies freq.
  127                             
  128  02:D6B2  09 80             ora             #$80                                    ; mark as noise set
  129  02:D6B4  8D 07 08          sta             psg_noise                               ; save noise frequency (?)
  130  02:D6B7  80 0F             bra             .noiseOK                                ; done setting noise
  131                     
  132                             ;..........................................................................
  133                             ; channel not noise capable, turn off noise
  134                             
  135  02:D6B9            .notNoise
  136                     
  137  02:D6B9  9C 07 08          stz             psg_noise                               ; no noise
  138  02:D6BC  BD E7 25          lda             psgSubFreqLo,x                  ; save tone frequency
  139  02:D6BF  8D 02 08          sta             psg_freqlo
  140  02:D6C2  BD ED 25          lda             psgSubFreqHi,x
  141  02:D6C5  8D 03 08          sta             psg_freqhi
  142                             
  143                             ;..........................................................................
  144                             ;       set volume pan register
  145                             
  146  02:D6C8            .noiseOK
  147  02:D6C8  BD F9 25          lda             psgSubPanPot,x                  ; get panning values
  148  02:D6CB  8D 05 08          sta             psg_pan                                 ; update register
  149                             
  150  02:D6CE  BD F3 25          lda             psgSubTrackVolume,x     ; get track volume
  151                             
  152  02:D6D1  38                sec
  153  02:D6D2  ED 15 26          sbc             psgSubFadeOutCount+1    ; minus fade-out stuff (?)
  154  02:D6D5  F0 02             beq             .voxMute                                ; if 0, mute voice
  155  02:D6D7  10 05         bpl         .voxVol                                 ; if > 0, set volume
  156                             
  157                             ;..........................................................................
  158                             ; set track volume
  159                             
  160  02:D6D9            .voxMute
  161  02:D6D9  9C 04 08          stz             psg_ctrl                                ; mute voice and stop channel
  162  02:D6DC  80 05             bra             .nextVoice
  163                     
  164  02:D6DE            .voxVol 
  165  02:D6DE  09 80             ora             #$80                                    ; turn on track play bit
  166  02:D6E0  8D 04 08          sta             psg_ctrl        
  167                             
  168                             ;..........................................................................
  169                             ; do next voice
  170                             
  171  02:D6E3            .nextVoice
  172  02:D6E3  CA                dex
  173  02:D6E4  30 03             bmi             .volExit                                ; if all done, exit
  174  02:D6E6  4C 33 D6          jmp             .voxLoop                                ; to top of loop
  175                     
  176  02:D6E9            .volExit
  177  02:D6E9  60                rts
