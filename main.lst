  142                     ; spr_pri(#flag)
  143                     ; ----
  144                     ; flag, new priority (1 = in foreground, 0 = in background)
  145                     
  146                             .macro spr_pri
  147                              ldy   #6
  148                              lda   [_si],Y
  149                              and   #$7F
  150                              ldx   \1
  151                              beq   .x\@
  152                              ora   #$80
  153                     .x\@:
  154                              sta [_si],Y
  155                             .endm
  156                     
  157                     
  158                     ; spr_pal(#index)
  159                     ; ----
  160                     ; index, palette index (0-15)
  161                     
  162                             .macro spr_pal
  163                              ldy   #6
  164                              lda   [_si],Y
  165                              and   #$F0
  166                              ora   \1
  167                              sta   [_si],Y
  168                             .endm
