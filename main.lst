   18                     ; bit 3 (ie $08) = 'run'
   19                     ; bit 4 (ie $10) = UP
   20                     ; bit 5 (ie $20) = RIGHT
   21                     ; bit 6 (ie $40) = DOWN
   22                     ; bit 7 (ie $80) = LEFT
