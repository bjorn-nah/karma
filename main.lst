  127                     ; spr_ctrl(#mask, #flag)
  128                     ; ----
  129                     ; mask, mask of the bits to change
  130                     ; flag, new bit value
  131                     
  132                             .macro spr_ctrl
  133                              ldy   #7
  134                              lda   \1
  135                              eor   #$FF
  136                              and   [_si],Y
  137                              ora   \2
  138                              sta   [_si],Y
  139                             .endm
