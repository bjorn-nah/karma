  111                             .macro spr_pattern
  112                              ldy   #4
  113                              .if (\?1 = ARG_IMMED)
  114                               lda   #LOW((\1) >> 5)
  115                               sta   [_si],Y
  116                               lda   #HIGH((\1) >> 5)
  117                              .else
  118                               lda   \1
  119                               sta   [_si],Y
  120                               lda   \1+1
  121                              .endif
  122                              iny
  123                              sta   [_si],Y
  124                             .endm
